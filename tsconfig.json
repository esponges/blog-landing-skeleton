{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "strict": true,
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Node",
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "jsxImportSource": "react"
  },
  "include": [
    ".astro/types.d.ts",
    "src/**/*",
    "src/types/*.d.ts"
  ],
  "exclude": [
    "dist",
    "node_modules"
  ]
}
